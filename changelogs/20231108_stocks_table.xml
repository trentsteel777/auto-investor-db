<?xml version="1.0" encoding="UTF-8"?>  
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:pro="http://www.liquibase.org/xml/ns/pro"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

	<changeSet id="1" author="trentsteel777" >
        <createTable tableName="STOCKS">
            <column name="ID" type="SERIAL">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="TICKER" type="VARCHAR(255)"/>
            <column name="COMPANY" type="VARCHAR(255)"/>
            <column name="SECTOR" type="VARCHAR(255)"/>
            <column name="INDUSTRY" type="VARCHAR(255)"/>
            <column name="COUNTRY" type="VARCHAR(255)"/>
            <column name="MARKET_CAP" type="VARCHAR(255)"/>
            <column name="PRICE_EARNINGS" type="VARCHAR(255)"/>
            <column name="PRICE" type="VARCHAR(255)"/>
            <column name="CHANGE" type="VARCHAR(255)"/>
            <column name="VOLUME"  type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATE" >
                <constraints nullable="false"/>
            </column>
            <column name="MODIFIED_DATE" type="DATE"/>
        </createTable>
        
		<rollback>
			<dropTable tableName="STOCKS"/>
		</rollback>
	</changeSet>
		
	<changeSet id="2" author="trentsteel777">
	    <insert tableName="STOCKS">
	        <column name="ID" valueNumeric="1"/>
	        <column name="TICKER" value="TESTT"/>
	        <column name="COMPANY" value="Test Inc."/>
	        <column name="SECTOR" value="Technology"/>
	        <column name="INDUSTRY" value="Consumer Electronics"/>
	        <column name="COUNTRY" value="United States"/>
	        <column name="MARKET_CAP" value="2.5T"/>
	        <column name="PRICE_EARNINGS" value="30.45"/>
	        <column name="PRICE" value="150.25"/>
	        <column name="CHANGE" value="+2.50"/>
	        <column name="VOLUME" value="10,000,000"/>
	        <column name="CREATED_DATE" valueDate="CURRENT_DATE"/>
	    </insert>
	    <rollback>
	        <delete tableName="STOCKS">
	            <where> TICKER='TESTT' </where>
	        </delete>
	    </rollback>
	</changeSet>

</databaseChangeLog>